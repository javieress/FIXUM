<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include('../partials/head') %>
        <title>
            <%= title %>
        </title>
</head>

<body>
    <%- include('../partials/navigation') %>
        <div class="container mt-5 " style="border-radius: 15px ; flex-direction: column;">
            <div class="card" style="border-radius: 15px ; flex-direction: column;">
                <div class="card-header p-4 justify-content-center align-items-center">
                    <h3>Últimos 10 activos agregados</h3>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th scope="col">ID</th>
                                    <th scope="col">Nombre</th>
                                    <th scope="col">Tipo</th>
                                    <!-- <th scope="col">Ubicación</th> -->
                                    <th scope="col">Encargado</th>
                                    <th scope="col">Cantidad</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for( let i in last10AssetAdded[0]) { %>
                                    <tr>
                                        <th scope="row">
                                            <%= last10AssetAdded[0][i].id %>
                                        </th>
                                        <td>
                                            <%= last10AssetAdded[0][i].asset_name %>
                                        </td>
                                        <td>
                                            <%= last10AssetAdded[0][i].assetType %>
                                        </td>
                                        <td>
                                            <%= last10AssetAdded[0][i].UserName %>
                                                <%= ' | ' + last10AssetAdded[0][i].nameUser %>
                                                    <%= last10AssetAdded[0][i].last_name %>
                                        </td>
                                        <td>
                                            <%= last10AssetAdded[0][i].quantity %>
                                        </td>
                                    </tr>

                                    <% } %>

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
</body>
<%- include('../partials/footer') %>

</html>